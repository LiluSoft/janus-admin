{
    "extends": [
        "tslint:recommended",
        "tslint-no-unused-expression-chai"
    ],
    "rules": {
        "no-console": [
            false
        ],
        "semicolon":true,
        "quotemark":true,
        "variable-name": false,
        "radix": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ],
        "indent": [
            true,
            "tabs"
        ],
        "max-line-length": [
            false
        ],
        "no-string-literal": false,
        "class-name": false,
        "no-namespace": [
            false
        ],
        "no-bitwise": false
    }
}
{
  "name": "@lilusoft/janus-admin",
  "version": "0.0.3",
  "description": "Janus Gateway Administration Module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bunyan": "^1.8.6",
    "@types/chai-subset": "^1.3.3",
    "@types/superagent": "^4.1.7",
    "@types/websocket": "^1.0.0",
    "amqplib": "^0.5.5",
    "bunyan": "^1.8.12",
    "chai-subset": "^1.6.0",
    "mqtt": "^3.0.0",
    "superagent": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "@types/bootbox": "^5.2.0",
    "@types/chai": "^4.1.6",
    "@types/jquery": "^3.3.33",
    "@types/jquery.blockui": "0.0.28",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.9.4",
    "@types/spin": "^2.3.30",
    "@types/toastr": "^2.1.37",
    "@types/webrtc": "0.0.26",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-browserify": "^0.5.1",
    "gulp-concat": "^2.6.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jquery-blockui": "^2.7.0",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "spin": "0.0.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typedoc": "^0.16.10",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webrtc-adapter": "^7.5.1"
  },
  "scripts": {
    "lint": "tslint --project .",
    "dryrun": "tsc -noEmit",
    "build": "rimraf ./dist/ && tsc",
    "unit-tests": "mocha -R spec --full-trace -r ts-node/register \"tests/**/*.spec.ts\" --exclude \"tests/**/*.integration.spec.ts\" | bunyan",
    "integration-tests": "mocha -R spec --full-trace -r ts-node/register tests/**/*.integration.spec.ts",
    "test": "mocha -R spec --full-trace -r ts-node/register tests/**/*.spec.ts",
    "simple": "ts-node ./tests/simple.ts",
    "test-videoroom": "ts-node tests/integration/videoroom.ts",
    "pre-commit": "npm run lint && npm run dryrun && npm run test",
    "doc": "rimraf ./docs/html && typedoc --tsconfig tsconfig.json",
    "doc-markdown": "rimraf ./docs/html && typedoc --tsconfig tsconfig.json --theme markdown",
    "coverage": "rimraf ./coverage && nyc mocha -r ts-node/register -r source-map-support/register --ui bdd tests/**/*.spec.ts",
    "publish-now": "npm publish --access public",
    "prepare-examples": "ts-node examples/prepare_token | bunyan",
    "start-videoroom": "webpack-dev-server --mode development  --config examples/videoroom.webpack.config.js"
  },
  "author": "Dror Gluska <drorgl@yahoo.com>",
  "license": "AGPL-3.0"
}
